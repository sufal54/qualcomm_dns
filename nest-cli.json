{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/qualcomm_dns2/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/qualcomm_dns2/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/qualcomm_dns2",
  "projects": {
    "dns": {
      "type": "application",
      "root": "apps/dns",
      "entryFile": "main",
      "sourceRoot": "apps/dns/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dns/tsconfig.app.json"
      }
    },
    "dns-management": {
      "type": "application",
      "root": "apps/dns-management",
      "entryFile": "main",
      "sourceRoot": "apps/dns-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dns-management/tsconfig.app.json"
      }
    }
  }
}